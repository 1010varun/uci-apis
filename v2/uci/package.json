{
  "name": "uci",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "migrate": "ts-node src/console.ts",
    "start": "nest start",
    "start:dev": "nest start --watch && yarn run generate-doc",
    "doc": "npx compodoc -p tsconfig.doc.json",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "generate-doc": "npx @compodoc/compodoc -p tsconfig.json -s",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@mittwald/vaults": "1.4.4",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^8.0.0",
    "@nestjs/microservices": "^8.4.3",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/platform-fastify": "^8.2.6",
    "@nestjs/swagger": "^5.2.0",
    "@prisma/client": "3",
    "add": "^2.0.6",
    "apollo-cache-inmemory": "^1.6.6",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "fastify-swagger": "^4.13.1",
    "graphql": "^16.3.0",
    "graphql-tag": "^2.12.6",
    "jest-mock-extended": "^2.0.5",
    "kafkajs": "^1.16.0",
    "lodash": "^4.17.21",
    "posthog-node": "^1.3.0",
    "prisma": "3",
    "prisma-class-generator": "^0.1.12",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.3.0",
    "undici": "^5.0.0",
    "uuid": "^8.3.2",
    "yarn": "^1.22.18"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.18",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.11.26",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "@vegardit/prisma-generator-nestjs-dto": "^1.5.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.5.1",
    "prettier": "^2.3.2",
    "supertest": "^6.2.2",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
